{
	"name": "@tuplo/split-names-list",
	"description": "Split lists of people's names",
	"version": "0.0.0-development",
	"repository": "git@github.com:tuplo/split-names-list.git",
	"author": "Rui Costa",
	"license": "MIT",
	"keywords": [],
	"files": [
		"dist/index.cjs",
		"dist/index.mjs",
		"dist/index.d.ts",
		"dist/split-names-list.d.ts"
	],
	"engines": {
		"node": ">=12"
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"exports": {
		".": [
			{
				"import": "./dist/index.mjs",
				"require": "./dist/index.cjs",
				"default": "./dist/index.mjs"
			},
			"./dist/index.mjs"
		]
	},
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "tsx ./sh/build.ts",
		"coverage": "tsx sh/coverage.ts",
		"format": "prettier --write src sh",
		"lint:ts": "tsc --noEmit",
		"lint": "eslint src",
		"test:ci": "vitest run",
		"test": "vitest --watch"
	},
	"devDependencies": {
		"@tuplo/shell": "1.2.0",
		"@types/node": "20.4.2",
		"@typescript-eslint/eslint-plugin": "6.0.0",
		"@typescript-eslint/parser": "6.0.0",
		"@vitest/coverage-v8": "0.33.0",
		"esbuild": "0.18.12",
		"eslint": "8.44.0",
		"eslint-config-airbnb-base": "15.0.0",
		"eslint-config-airbnb-typescript": "17.0.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-import": "2.27.5",
		"nyc": "15.1.0",
		"prettier": "3.0.0",
		"tsx": "3.12.7",
		"typescript": "5.1.6",
		"vitest": "0.33.0"
	},
	"release": {
		"branches": [
			"main"
		]
	}
}
